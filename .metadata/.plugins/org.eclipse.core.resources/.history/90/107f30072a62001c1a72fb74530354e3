/*===============================
	RegionL.java
	- 지역 대분류 DTO
================================*/

package com.test.mvc;

// 지역테이블 2개해서 대분류 소분류 나누기!
public class RegionL
{
	// 주요 속성 구성
	private String regionLId, city;

	// getter / setter 구성
	public String getRegionId()
	{
		return regionLId;
	}

	public void setRegionId(String regionLId)
	{
		this.regionLId = regionLId;
	}

	public String getCity()
	{
		return city;
	}

	public void setCity(String city)
	{
		this.city = city;
	}
	
}
